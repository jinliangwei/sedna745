%symbol {
  identifier : "ratings",
  type: ndarray<2, float>,
  init_file : "ratings_file"
}

%symbol {
  identifier : "L",
  type: ndarray<2, float>,
  dims : [@symbol_table["ratings"].shape[0]]
}

%symbol {
  identifier : "R",
  type: ndarray<2, float>,
  dims : [@symbol_table["ratings"].shape[1]]
}

%symbol {
  identifier : "main",
  type : function,
  return_type : void,
  argument_types : []
}

%function {
  %attributes {
    identifier: "main"
  }

  %symbol {
    identifier : "eta",
    type : float,
    init_value : 0.1
  }

  %symbol {
    identifier : "lambda",
    type : float,
    init_value : 0.01
  }

  %symbol {
    identifier : "K",
    type : int8,
    init_value : 100
  }

  @iterate_for {
    %attributes {
       num_iterations: 100
    }
    @foreach {
      %attributes {
        iteration_space: @symbol_table["ratings"],
        iteration_element: @symbol_table["e"]
      }
      %symbol {
        identifier : "pred",
	type : float
      }

      %symbol {
        identifier : "e",
	type : float
      }

      %symbol {
        identifier : "gradc",
	type : float
      }

      %symbol {
        identifier : "L_grad",
	type : float
      }

      %symbol {
        identifier : "R_grad",
	type : float
      }

      @load t0 @symbol_table["e"]
      @member_read t1 t0 (idx)
      @array_read t2 t1 (1)
      @load t3 @symbol_table["R"]
      @ndarray_read t4 t3 (t2)
      @call t5 t4 transpose ()
      @array_read t6 t1 (0)
      @load t7 @symbol_table["L"]
      @ndarray_read t8 t7 (t6)
      @call t9 t8 dot_prod (t4)
      @store @symbol_table["pred"] t9

      @member_read t10 @symbol_table["e"] (val)
      @subtract t11 t9 t10
      @multiply t12 -2 t11
      @store @symbol_table["gradc"] t12

      @multiply t13 t12 t2
      @load t14 @symbol_table["lambda"]
      @multiply t15 t14 t6
      @add t16 t12 t15
      @store @symbol_table["L_grad"] t16

      @multiply t17 t12 t6
      @multiply t18 t14 t2
      @add t19 t17 t18
      @store @symbol_table["R_grad"] t19

      @load t20 @symbol_table["eta"]
      @multiply t21 t20 t16
      @subtract t22 t8 t21
      @ndarray_store @symbol_table["L"] t22 (t6)

      @multiply t23 t20 t19
      @subtract t24 t4 t23
      @ndarray_store @symbol_table["R"] t24 (t2)
    }
  }
}
